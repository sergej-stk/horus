{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "src/Horus.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "src/Horus.csproj",
                "--configuration",
                "Release",
                "--output",
                "src/bin/Release/net7.0/publish"
            ],
            "problemMatcher": []
        }
    ]
}
